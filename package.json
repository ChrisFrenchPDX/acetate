{
  "name": "acetate",
  "description": "Layout and templating framework for static websites.",
  "version": "0.3.1",
  "author": "Patrick Arlt",
  "bugs": {
    "url": "https://github.com/patrickarlt/acetate/issues"
  },
  "dependencies": {
    "async": "~1.4.2",
    "browser-sync": "~2.9.1",
    "chalk": "~1.0.0",
    "chokidar": "~1.2.0",
    "glob": "~5.0.5",
    "highlight.js": "~8.8.0",
    "js-yaml": "~3.4.2",
    "lodash": "~3.10.1",
    "markdown-it": "~5.0.0",
    "minimatch": "~3.0.0",
    "mkdirp": "~0.5.1",
    "normalize-newline": "~1.0.2",
    "nunjucks": "~2.1.0",
    "portscanner": "~1.0.0",
    "rimraf": "~2.4.0",
    "yargs": "~3.4.5"
  },
  "devDependencies": {
    "codeclimate-test-reporter": "0.1.1",
    "gh-release": "~2.0.1",
    "istanbul": "~0.3.13",
    "request": "~2.55.0",
    "semistandard": "*",
    "shelljs": "~0.5.1",
    "snazzy": "~2.0.1",
    "tap-spec": "^4.1.0",
    "tape": "~4.0.0"
  },
  "homepage": "http://acetate.io",
  "keywords": [
    "site",
    "static",
    "template",
    "templating"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:patrickarlt/acetate.git"
  },
  "scripts": {
    "prepublish": "npm test",
    "release": "npm publish && gh-release",
    "test": "npm run lint && tape test/**/test.js | tap-spec",
    "test:cover": "istanbul cover ./node_modules/tape/bin/tape --dir ./coverage/folder -- test/**/test.js",
    "lint": "semistandard | snazzy"
  }
}

{
  "name": "acetate",
  "description": "Layout and templating framework for static websites.",
  "version": "0.2.0",
  "author": "Patrick Arlt",
  "bugs": {
    "url": "https://github.com/patrickarlt/acetate/issues"
  },
  "dependencies": {
    "async": "~0.9.0",
    "chalk": "~1.0.0",
    "chokidar": "~1.0.0",
    "glob": "~5.0.5",
    "highlight.js": "~8.5.0",
    "js-yaml": "~3.2.7",
    "lodash": "~3.7.0",
    "markdown-it": "~4.2.0",
    "minimatch": "~2.0.4",
    "mkdirp": "~0.5.0",
    "node-static": "~0.7.3",
    "nunjucks": "~1.3.3",
    "opener": "^1.4.0",
    "portscanner": "^1.0.0",
    "rimraf": "~2.3.2",
    "yargs": "^3.4.5"
  },
  "devDependencies": {
    "codeclimate-test-reporter": "0.0.4",
    "gh-release": "^1.1.7",
    "istanbul": "^0.3.13",
    "semistandard": "*",
    "tap-spec": "^3.0.0",
    "tape": "^4.0.0"
  },
  "homepage": "http://acetate.io",
  "keywords": [
    "site",
    "static",
    "template",
    "templating"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:patrickarlt/acetate.git"
  },
  "scripts": {
    "prepublish": "npm test",
    "release": "npm publish && gh-release",
    "test": "npm run lint && npm run test:unit && npm run test:features && istanbul report",
    "test:unit": "istanbul cover ./node_modules/tape/bin/tape --dir ./coverage/unit -- test/*.js | tap-spec",
    "test:features": "istanbul cover ./node_modules/tape/bin/tape --dir ./coverage/features -- test/features/*/test.js | tap-spec",
    "test:dev": "tape test/features/*/test.js",
    "lint": "semistandard"
  }
}
